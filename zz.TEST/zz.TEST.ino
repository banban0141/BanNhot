void setup()
{
  // Thiết lập các chân 11, 10, 9 làm đầu ra
  pinMode(11, OUTPUT);
  pinMode(10, OUTPUT);
  pinMode(9, OUTPUT);

  // Kích mức cao (HIGH) các chân 11, 10, 9
  digitalWrite(11, HIGH);
  digitalWrite(10, HIGH);
  digitalWrite(9, HIGH);
}

